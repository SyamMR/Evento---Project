<section>
    <div class="container p-1 pt-5 mt-5">
      <div>
        <h3 class="pt-3 pb-3">Users Feedbacks</h3>
      </div>
        <table class="table">
  <thead>
    <tr>
      <th scope="col" >User Name</th>
      <th scope="col">Email</th>
      <th scope="col">Message</th>
    </tr>
  </thead>
  <tbody>
    {{#each feedbacks}}
    <tr>
      <td>{{this.Name}}</td>
      <td>{{this.Email}}</td>
      <td>{{this.Message}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>
</section>